{
	"scopeName": "source.ula",
	"patterns": [{ "include": "#expression" }],
	"repository": {
	  "expression": {
		"patterns": [
			{ "include": "#keywords"},
			{ "include": "#constants" },
			{ "include": "#Operators"},
			{ "include": "#types" },
			{ "include": "#look_read" },
			{ "include": "#identificador" },
			{ "include": "#Digit" },
			{ "include": "#paren-expression"}
		]
	  },
	  "constants": {
		"match": "falso|verdadero|nulo|infinito|FALSO|VERDADERO|NULO|INFINITO",
		"name": "keyword.letter"
	  },
	  "look_read": {
		"match": "mostrar|leer",
		"name": "color"
	  },
	  "keywords": {
		"match": "variable|para|mientras|segun_sea|salir|sino|si|caso|defecto|defer|operator|extern|def|const|super|self|retorna|intenta|atrapa|tirar|finalmente|yield|fallthrough|continuar|del|from|import|with|pass|void|type|override|abstract|assert",
		"name": "key"
	  },
	  "identificador" :{
		 "match": "[a-zA-Z_][a-zA-Z_0-9]*",
		 "name" : "variable"
	  },
	  "Digit": {
		"match" : "[0-9]+",
		"name" : "constant.numeric"
	  },
	  "Operators": {
		"match": ":=|\\<|\\>|\\+|\\-|\\*|/|=|!=|%|\\^|<<<|>>>|xor|~|<<|>>|y_es|o_es|no|es|en|\\,|\\:|\\.|\\..|\\...|->|as|\\+=|\\-=|\\*=|=|%=|^=|\\++|\\--|==|!|<=|>=|@",
		"name" : "comment"
	},
	 "types": {
		"match": "any|Entero|Entero8|Entero16|Entero32|Entero64|Entero128|Sin_signo|Sin_signo8|Sin_signo16|Sin_signo32|Sin_signo64|Sin_signo128|doble|flotante|complejo|cadena|booleano|lista|tupla|conjunto|dict|Enum|fun|Estructura|clase",
		"name" : "entity.name.tag"
	 },
	  "paren-expression": {
		"begin": "\\(|\\{|\\[",
		"end": "\\)|\\}|\\]",
		"beginCaptures": {
		  "0": { "name": "punctuation.paren.open" }
		},
		"endCaptures": {
		  "0": { "name": "punctuation.paren.close" }
		},
		"name": "expression.group",
		"patterns": [{ "include": "#expression" }]
	  }
	}
  }